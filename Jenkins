pipeline {
    agent any
    stages {
        stage(checkout) {
            steps {
                git branch: 'main', credentialsId: '688b0848-389f-4a39-a6b2-9c8e80ee4830', url: 'https://github.com/Saranraj-25/example.git'
            }
        }
        stage('Test') {
            steps {
                echo "testing the code "
            }
        }
        stage('Deploy') {
            steps {
                echo " deploying the cide "
            }
        }
    }
}
